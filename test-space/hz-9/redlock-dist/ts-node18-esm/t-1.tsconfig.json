{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2021",
    "module": "Node16",
    "moduleResolution": "Node16",
    "baseUrl": "./src",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./lib",
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": false
  }
}
